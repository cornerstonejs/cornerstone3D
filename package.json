{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12",
    "yarn": ">=1.19.1"
  },
  "scripts": {
    "build": "lerna run build:package --stream",
    "build:demo": "lerna run build:demo --stream",
    "dev": "lerna run dev:demo --stream",
    "docs": "lerna run docs",
    "docs:watch": "lerna run docs:watch",
    "preinstall": "node preinstall.js",
    "start": "yarn run dev",
    "test:firefox": "karma start ./karma.conf.js --browsers Firefox",
    "test:dev": "karma start",
    "test:ci": "karma start --single-run",
    "test": "karma start",
    "lint-staged": "lint-staged",
    "lint": "eslint --quiet -c .eslintrc.json packages/**/src",
    "predeploy": "yarn install && yarn run build:release",
    "prepublishOnly": "yarn run build && yarn run build:release",
    "compile": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "^4.5.0",
    "classnames": "^2.3.0"

  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/node": "^14.18.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "calculate-suv": "git+ssh://git@github.com/PrecisionMetrics/calculate-suv.git#main",
    "chai": "^4.3.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "cornerstone-wado-image-loader": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cssnano": "^4.1.11",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-webpack-plugin": "^2.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.99.0",
    "karma": "^6.3.12",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.33",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^10.4.0",
    "shader-loader": "^1.3.1",
    "sinon": "^10.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^4.0.0",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.11",
    "typedoc-plugin-pages-fork": "^0.0.1",
    "typescript": "4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "5.8.0",
    "worker-loader": "^3.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
