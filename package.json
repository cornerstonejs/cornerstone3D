{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12",
    "yarn": ">=1.19.1"
  },
  "scripts": {
    "build": "lerna run build:package --stream",
    "build:demo": "lerna run build:demo --stream",
    "dev": "lerna run dev:demo --stream",
    "docs": "lerna run docs",
    "docs:watch": "lerna run docs:watch",
    "preinstall": "node preinstall.js",
    "start": "yarn run dev",
    "test": "NODE_ENV=test karma start",
    "test:firefox": "karma start ./karma.conf.js --browsers Firefox",
    "test:ci": "karma start --single-run",
    "lint-staged": "lint-staged",
    "lint": "eslint --quiet -c .eslintrc.json packages/**/src",
    "predeploy": "yarn install && yarn run build:release",
    "prepublishOnly": "yarn run build && yarn run build:release",
    "compile": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "calculate-suv": "git+ssh://git@github.com/PrecisionMetrics/calculate-suv.git#main",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "cornerstone-wado-image-loader": "^3.3.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-webpack-plugin": "^2.5.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.6",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.7.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "puppeteer": "^10.1.0",
    "shader-loader": "^1.3.1",
    "sinon": "^10.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "ts-loader": "^9.1.2",
    "typedoc": "^0.21.4",
    "typedoc-plugin-pages-fork": "^0.0.1",
    "typescript": "4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "5.7.3",
    "worker-loader": "^3.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
