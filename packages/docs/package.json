{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "node preinstall.js",
    "copy-coverage-to-docs": "cp -R ../../coverage/*Chrome*/. ./static/static-coverage/",
    "docusaurus": "docusaurus",
    "docs": "docusaurus start",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "docs:ci": "cd ../.. && yarn run test:ci && cd packages/docs && yarn run copy-coverage-to-docs && yarn run build",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.3",
    "@docusaurus/preset-classic": "2.0.0-beta.3",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "plugin-image-zoom": "github:ataft/plugin-image-zoom",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "^0.15.2",
    "docusaurus-plugin-copy": "^0.1.1",
    "typedoc": "^0.21.4",
    "typedoc-plugin-markdown": "^3.10.3",
    "copyfiles": "2.4.1",
    "puppeteer": "^10.1.0",
    "karma-chrome-launcher": "^3.1.0"
  }
}
