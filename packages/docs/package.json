{
  "name": "docs",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "preinstall": "node preinstall.js",
    "copy-coverage-to-docs": "cp -R ../../coverage/*Chrome*/. ./static/test-coverage/",
    "copy-examples-to-docs": "cp -R ../../.static-examples/. ./static/examples/",
    "docusaurus": "docusaurus",
    "docs": "cross-env NODE_OPTIONS=--max_old_space_size=16384 docusaurus start",
    "start": "yarn run docs",
    "dev": "yarn run docs",
    "dev:watch": "cross-env NODE_OPTIONS=--max_old_space_size=16384 TYPEDOC_WATCH=true docusaurus start",
    "build:docs": "cross-env NODE_OPTIONS=--max_old_space_size=16384 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "prep-coverage": "cd ../.. && yarn run test:ci && cd packages/docs && yarn run copy-coverage-to-docs",
    "prep-examples": "cd ../.. && yarn run build-all-examples && cd packages/docs && yarn run copy-examples-to-docs",
    "prep": "yarn run prep-coverage && yarn run prep-examples",
    "docs:ci": "cross-env NODE_OPTIONS=--max_old_space_size=16384 && yarn run prep && yarn run build:docs",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.17",
    "@docusaurus/module-type-aliases": "2.0.0-beta.17",
    "@docusaurus/preset-classic": "2.0.0-beta.17",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^6.2.1",
    "clsx": "^1.1.1",
    "docusaurus-plugin-copy": "^0.1.1",
    "docusaurus-plugin-typedoc-api": "^1.9.0",
    "file-loader": "^6.2.0",
    "plugin-image-zoom": "github:ataft/plugin-image-zoom",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "2.4.1",
    "esbuild-loader": "^2.18.0",
    "karma-chrome-launcher": "^3.1.0",
    "netlify-plugin-cache": "^1.0.3",
    "puppeteer": "^13.1.3",
    "typedoc": "^0.22.13"
  }
}
