{
  "name": "dcmjs",
  "version": "0.2.1",
  "description": "Javascript implementation of DICOM manipulation",
  "main": "build/dcmjs.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run webpack:prod",
    "build:dev": "npm run webpack:dev",
    "watch": "npm run webpack:watch",
    "webpack:dev": "webpack --progress --config ./config/webpack/webpack-dev",
    "webpack:prod": "webpack --progress --config ./config/webpack/webpack-prod",
    "webpack:watch": "webpack --progress --debug --watch  --config ./config/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pieper/dcmjs.git"
  },
  "author": "Steve Pieper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pieper/dcmjs/issues"
  },
  "homepage": "https://github.com/pieper/dcmjs#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "cornerstone-core": "^1.1.0",
    "cornerstone-math": "^0.1.6",
    "cornerstone-tools": "^1.0.3",
    "cornerstone-wado-image-loader": "^1.0.1",
    "hammerjs": "^2.0.8",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0"
  }
}
