{
  "name": "@precisionmetrics/cornerstone-tools",
  "version": "0.0.4-rc13",
  "description": "Cornerstone-3D tools",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev:core": "yarn run dev",
    "build": "yarn run build:webpack && yarn run build:lib && yarn run build:lib-esm",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:lib-esm": "tsc --project ./tsconfig.build.json -m es6 --outDir ./dist/lib-esm",
    "build:webpack": "cross-env NODE_ENV=production webpack --config .webpack/webpack.prod.js",
    "build:package": "yarn run build",
    "example": "node ../../utils/ExampleRunner/example-runner-cli.js",
    "prepublishOnly": "yarn run build",
    "start": "yarn run dev"
  },
  "peerDependencies": {
    "gl-matrix": "^3.3.0",
    "vtk.js": "git+ssh://git@github.com/PrecisionMetrics/MGH-BC-vtk-js.git#fix/labelMapOutline",
    "@precisionmetrics/cornerstone-image-loader-streaming-volume": "^0.0.4-rc13"
  },
  "devDependencies": {
    "vtk.js": "git+ssh://git@github.com/PrecisionMetrics/MGH-BC-vtk-js.git#fix/labelMapOutline",
    "@precisionmetrics/cornerstone-image-loader-streaming-volume": "^0.0.4-rc13"
  },
  "author": "",
  "license": "ISC"
}
